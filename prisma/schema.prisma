generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Book {
  id                        String   @id @default(uuid())
  date_of_creation          DateTime @default(now())
  email                     String
  url                       String   @unique
  tel                       String
  age                       Int
  color                     String
  okey                      String   @unique
  city                      String
  author                    String
  number_of_pages           Int
  number_of_chapters        Int
  number_of_published_books Int
}
